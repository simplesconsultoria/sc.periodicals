<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="here/main_template/macros/master"
      i18n:domain="plone">

<body>

<metal:content-core fill-slot="content-core">
<metal:block use-macro="context/standard_view/macros/content-core">

    <metal:entries fill-slot="entries">
    <metal:block use-macro="context/standard_view/macros/entries">
    <metal:entry fill-slot="entry">

        <div class="image-left visualIEFloatFix"
             tal:define="obj item/getObject">
            <a href="#"                  tal:attributes="href item/getURL">
          
                 
 
             <div class="tileImage" tal:define="image python:obj.image">
                <img tal:define="scale obj/@@images" 
                     tal:replace="structure python: scale.scale('image', scale='thumb').tag()"/>
             </div>

            </a>

            <div class="visualClear"><!-- --></div>
        </div>




    </metal:entry>
    </metal:block>
    </metal:entries>

</metal:block>
</metal:content-core>

</body>
</html>
